  9 void puts2(char *str)
 10 
 11 {
 12         int i;
 13         int j = 0;
 14 
 15         while (str[j] != '\0')
 16         {
 17                 j++;
 18         }
 19 
 20         for (i = 0; i < j; i += 2)
 21         {
 22                 _putschar(str[i]);
 23         }
 24                 _putchar('\n');
